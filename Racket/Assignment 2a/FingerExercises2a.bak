;Exercise 1
(define (sub2 x)
  (cond
    [(number? x) (- x 2)]
    [(not (number? x)) (error "no no mister superman")]
    )
  )

;Exercise 2
(define (rdist t)
  (cond
    [(and (number? t) (>= t 0)) (+ (* 1.5 t) 1.5)]
    )
  )

